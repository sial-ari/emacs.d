#!/usr/bin/env python

x = 10
itersLeft = x
biggestOdd = 0
while (itersLeft != 0):
    num = raw_input("Insert number" + itersLeft + ": ")
    if num % 2 != 0:
        if num > biggestOdd:
            biggestOdd = num
    itersLeft = itersLeft - 1

if biggestOdd == 0:
    print "There were no even numbers!"
else:
    print "The biggest odd number was: " + biggestOdd



